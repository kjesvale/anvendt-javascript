{
    "name": "anvendt-javascript",
    "description": "En workshop for frontendprogrammet",
    "version": "1.0.0",
    "scripts": {
        "prestart": "rm -rf dist",
        "start": "parcel src/*.html",
        "start:solution": "parcel solution/*.html",
        "prebuild": "rm -rf dist",
        "build": "parcel build src/*.html",
        "build:solution": "parcel build solution/*.html",
        "reset": "rm -rf dist .parcel-cache"
    },
    "devDependencies": {
        "parcel": "^2.11.0",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3"
    },
    "prettier": {
        "tabWidth": 4
    }
}
